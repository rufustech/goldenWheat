{
    "version": 2,
    "builds": [
      {
        "src": "cpnt262-the-golden-wheat-frontend/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "CPNT-262-Golden-Wheat-Bakery-backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/CPNT-262-Golden-Wheat-Bakery-backend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/cpnt262-the-golden-wheat-frontend/$1"
      }
    ]
  }
  